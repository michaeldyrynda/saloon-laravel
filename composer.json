{
    "name": "sammyjo20/saloon-laravel",
    "description": "Laravel package for Sammyjo20/Saloon",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "sammyjo20",
        "saloon",
        "sdk",
        "api",
        "api-integrations"
    ],
    "authors": [
        {
            "name": "Sam Carr√©",
            "email": "29132017+Sammyjo20@users.noreply.github.com",
            "role": "Developer"
        },
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/sammyjo20/saloon-laravel",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/michaeldyrynda/saloon"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "sammyjo20/saloon": "dev-v1-php74"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.24 || ^7.7",
        "pestphp/pest": "^1.21",
        "rector/rector": "^0.14.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Sammyjo20\\SaloonLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sammyjo20\\SaloonLaravel\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Saloon": "Saloon"
            },
            "providers": [
                "Sammyjo20\\SaloonLaravel\\SaloonServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix"
        ],
        "scripts": {
            "post-autoload-dump": [
                "@php ./vendor/bin/testbench package:discover --ansi"
            ]
        },
        "test": [
            "./vendor/bin/pest"
        ]
    }
}
